{
	"name": "node-zaim",
	"version": "0.1.0",
	"description": "Node.js client library for Zaim API",
	"homepage": "https://github.com/ryohidaka/node-zaim#readme",
	"bugs": {
		"url": "https://github.com/ryohidaka/node-zaim/issues"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ryohidaka/node-zaim.git"
	},
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch",
		"prepare": "bun simple-git-hooks",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"release": "bumpp --commit --push --tag",
		"test": "bun test",
		"test:coverage": "bun test --coverage",
		"test:watch": "bun test --watch",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@types/bun": "^1.3.7",
		"@types/oauth": "^0.9.6",
		"bumpp": "^10.4.0",
		"bunup": "^0.16.20",
		"readline": "^1.3.0",
		"simple-git-hooks": "^2.13.1",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"module": "./dist/index.js",
	"simple-git-hooks": {
		"pre-commit": "bun run lint && bun run type-check"
	},
	"types": "./dist/index.d.ts",
	"dependencies": {
		"oauth": "^0.10.2",
		"zod": "^4.3.6"
	}
}
